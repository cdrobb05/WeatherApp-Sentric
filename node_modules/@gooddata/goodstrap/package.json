  "scripts": {
    "prepublish": "rm -rf lib && gulp",
    "test": "jest --watch",
    "test-once": "jest",
    "stylelint": "stylelint 'src/**/*.scss'",
    "validate": "tsc --noEmit && yarn eslint && yarn stylelint && yarn prettier-check",
    "eslint": "eslint --ext=js,jsx,ts,tsx src/",
    "validate-style-ci": "stylelint 'src/**/*.scss' --custom-formatter=node_modules/stylelint-checkstyle-formatter > ci/results/stylelint-results.xml",
    "storybook": "start-storybook -p 9001 -c .storybook -s .",
    "build-storybook": "rm -rf dist-storybook && build-storybook -c .storybook -o dist-storybook && cp -R src dist-storybook/",
    "prettier-check": "prettier --check './**/*.{ts,js,tsx,jsx,json,scss,md,yaml,html}'",
    "prettier-write": "prettier --write './**/*.{ts,js,tsx,jsx,json,scss,md,yaml,html}'"
  },
