  "lint-staged": {
    "src/**/*.{ts,js,tsx,jsx,scss}": [
      "npx gdc-fix-copyright",
      "git add"
    ],
    "./**/*.{ts,js,tsx,jsx,json,scss,md,yaml,html}": [
      "prettier --write",
      "git add"
    ]
  }
